-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var height_equal  = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Ui Kits','Progress Bar'],'Progress Bar')
            // Container-fluid starts
            .container-fluid
              .row 
                .col-xl-12
                  include ../../components/Ui-kits/progress/basic-progress
                .col-lg-6
                  include ../../components/Ui-kits/progress/stripped-progress
                .col-lg-6
                  include ../../components/Ui-kits/progress/stripped-animated
                .col-lg-6
                  include ../../components/Ui-kits/progress/multiple-bar
                .col-lg-6
                  include ../../components/Ui-kits/progress/step-progress
                .col-xl-12
                  include ../../components/Ui-kits/progress/custom-progress
                .col-lg-6
                  include ../../components/Ui-kits/progress/small-progress
                .col-lg-6
                  include ../../components/Ui-kits/progress/large-progress
                .col-xl-12
                  include ../../components/Ui-kits/progress/height-progress
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files