-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var quill_editor = true; 
-var custom_quill = true;
-var bubble_quill = true; 
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Editors','Quilljs Editor'],'Quilljs Editor')
            // Container-fluid starts
            .container-fluid
              .row
                .col-sm-12
                  include ../../components/Editors/quill-editor/default-quill-editor
                .col-sm-12
                  include ../../components/Editors/quill-editor/snow-editor
                .col-sm-12
                  include ../../components/Editors/quill-editor/bubble-editor
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files