-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Blog','Blog Details'],'Blog Details')
            // Container-fluid starts
            .container-fluid
                 .row 
                    .col-xl-6.set-col-12.box-col-12
                        include ../../components/Blog/blog-details/blog-components.pug
                    .col-xl-6.set-col-12.box-col-12
                        include ../../components/Blog/blog-details/blog-components-two.pug
                    .col-md-6.col-xxl-3.box-col-6
                        include ../../components/Blog/blog-details/blog-components-three.pug
                    .col-md-6.col-xxl-3.box-col-6
                        include ../../components/Blog/blog-details/blog-components-four.pug
                    .col-md-6.col-xxl-3.box-col-6
                        include ../../components/Blog/blog-details/blog-components-five.pug
                    .col-md-6.col-xxl-3.box-col-6
                        include ../../components/Blog/blog-details/blog-components-six.pug
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files