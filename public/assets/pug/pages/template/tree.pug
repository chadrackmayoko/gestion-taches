-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var tree          = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Bonus Ui','Tree View'],'Tree View')
            // Container-fluid starts
            .container-fluid
                .row 
                    .col-xl-6
                      .card
                        .card-header.pb-0
                            +card-header('Basic Tree', '', '', 'Use the dynamic tree view with checkboxes.')
                        .card-body
                            .tree-container
                    .col-xl-6
                        .card
                          .card-header.pb-0
                              +card-header('Disabled Tree','','', 'Use the dynamic tree view with checkboxes.')
                          .card-body
                              .disabled-container
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files