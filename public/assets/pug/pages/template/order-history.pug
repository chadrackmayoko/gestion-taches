-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var datatables = true;
-var product_list = true;
-var order_product = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Ecommerce','Recent Orders'],'Recent Orders')
            // Container-fluid starts
            .container-fluid.order-history-page
                .row 
                  .col-sm-12
                    include ../../components/Ecommerce/recent-orders/new-orders
                  .col-sm-12
                    include ../../components/Ecommerce/recent-orders/datatable-order
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files