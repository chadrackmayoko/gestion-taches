-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var choice = true;
-var typehead = true;
-var height_equal = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header/header
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            +breadcrumb(['Form Widgets','Typeahead'],'Typeahead')
          // Container-fluid starts
          .container-fluid.typeahead-page
            .row 
              .col-sm-12.col-md-6
                include ../../components/forms/typeahead/basic_typeahead
              .col-sm-12.col-md-6
                include ../../components/forms/typeahead/prefetch
              .col-sm-12.col-md-6
                include ../../components/forms/typeahead/bloodhound 
              .col-sm-12.col-md-6
                include ../../components/forms/typeahead/remote_typeahead
              .col-sm-12.col-md-6
                include ../../components/forms/typeahead/multiple_with_headers
              .col-sm-12.col-md-6
                include ../../components/forms/typeahead/rtl_support
          // Container-fluid starts
        include ../../components/footer
    include ../../components/footer-files