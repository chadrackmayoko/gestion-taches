-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var select2 = true;
-var md_sidebar = true;
-var page_select2 = true;
-var bookmark_page = true;
-var validation_form = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Apps','Bookmarks'],'Bookmarks')
            // Container-fluid starts
            .container-fluid.email-wrap.bookmark-wrap
                 .row 
                    .col-xl-3.box-col-6
                        include ../../components/apps/bookmarks/bookmark-left-sidebar
                    .col-xl-9.col-md-12.box-col-12
                        include ../../components/apps/bookmarks/bookmark-right-sidebar
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files