-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var md_sidebar = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Job Search','List View'],'List View')
            // Container-fluid starts
            .container-fluid.job-cards-view
              .row 
                .col-xxl-3.col-xl-4.box-col-30
                  include ../../components/Job-search/list-view/list-view-components
                .col-xxl-9.col-xl-8.box-col-12
                  include ../../components/Job-search/list-view/list-components
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files