-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var swiper                = true;
-var datatables         = true;
-var apexcharts         = true;
-var dashboard_2        = true;
include ../../components/mixin
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Dashboard','Ecommerce Dashboard'],'Ecommerce Dashboard')
            // Container-fluid starts
            .container-fluid.ecommerce-dashboard
              .row 
                .col-xl-9
                    .row
                        include ../../components/dashboard2/order-delivery
                        .col-md-6.col-xl-7
                            include ../../components/dashboard2/selling-product
                        .col-md-6.col-xl-5
                            include ../../components/dashboard2/category-overview
                .col-md-6.col-xl-3
                    include ../../components/dashboard2/manage-order
                .col-md-6
                    include ../../components/dashboard2/sales-summary
                .col-md-6.col-xl-3
                    include ../../components/dashboard2/product-offer
                .col-md-6.col-xl-3
                    include ../../components/dashboard2/sale-progress
                .col-md-12.col-xl-4
                    include ../../components/dashboard2/best-seller
                .col-md-6.col-xl-4
                    include ../../components/dashboard2/discount-product
                    .col-xl-12
                        include ../../components/dashboard2/track-order
                .col-md-6.col-xl-4
                    include ../../components/dashboard2/saleby-product
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files