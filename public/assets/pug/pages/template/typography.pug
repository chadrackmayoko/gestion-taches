-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Ui Kits','Typography'],'Typography')
            // Container-fluid starts
            .container-fluid
              .row 
                  .col-xxl-8.col-xl-12
                    include ../../components/Ui-kits/typography/heading
                  .col-xxl-4.col-xl-12
                    include ../../components/Ui-kits/typography/color-heading
                  .col-xl-12
                    include ../../components/Ui-kits/typography/font-weight
                  .col-xl-12
                    include ../../components/Ui-kits/typography/listing-typography
                  .col-xl-12
                    include ../../components/Ui-kits/typography/display-heading
                  .col-xxl-6.col-xl-12
                    include ../../components/Ui-kits/typography/inline-typography
                  .col-xxl-6.col-xl-12
                    include ../../components/Ui-kits/typography/text-color
                  .col-xl-12
                    include ../../components/Ui-kits/typography/blockquote
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files