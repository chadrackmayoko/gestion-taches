-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var photoswipe = true;
-var photoswipe_caption = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid
              +breadcrumb(['Apps','Social App'],'Social App')
            // Container-fluid starts
            .container-fluid.user-profile.social-app-profile
              .row
                .col-sm-12.box-col-12
                  include ../../components/apps/social-app/social-profile
              #top-tabContent.tab-content
                include ../../components/apps/social-app/social-app-component
            // Container-fluid ends
          include ../../components/footer
    include ../../components/footer-files