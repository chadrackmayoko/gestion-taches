-var sidebar                = true;
-var theme_customizer       = true;
-var scrollbar              = true;
-var Icons_copy   = true;
include ../../components/mixin
doctype html
html(lang='en') 
  include ../../components/header-files
  body
    include ../../components/tap-top
    include ../../components/loader
    main#pageWrapper.page-wrapper.compact-wrapper
        include ../../components/header/header
        .page-body-wrapper
          include ../../components/sidebar
          .page-body
            .container-fluid  
              +breadcrumb(['Project','Project List'],'Project List')
            // Container-fluid starts
            .container-fluid
              .row.project-cards 
                .col-md-12.project-list
                   include ../../components/apps/project/project-tablist.pug
                .col-sm-12 
                  include ../../components/apps/project/project-card.pug
            // Container-fluid starts   
          include ../../components/footer  
    include ../../components/footer-files