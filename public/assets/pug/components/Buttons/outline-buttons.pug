        },
        {
            title: 'Tertiary Button',
            BtnColor: 'btn-tertiary btn-outline-tertiary',
            tooltip: 'btn btn-outline-tertiary',
        },
        {
            title: 'Success Button',
            BtnColor: 'btn-success btn-outline-success',
            tooltip: 'btn btn-outline-success',
        },
        {
            title: 'Info Button',
            BtnColor: 'btn-info btn-outline-info',
            tooltip: 'btn btn-outline-info',
