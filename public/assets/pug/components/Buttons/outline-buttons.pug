            title: 'Primary Button',
            BtnColor: 'btn-primary btn-outline-primary',
            tooltip: 'btn btn-outline-primary',
        },
        {
            title: 'Secondary Button',
            BtnColor: 'btn-secondary btn-outline-secondary',
