//-// Travel Icon
-
    var ICO_ITEM27 =[
       { id:'icofont-5-star-hotel' }
        , { id:'icofont-air-ticket' }
        , { id:'icofont-beach-bed' }
        , { id:'icofont-beach' }
        , { id:'icofont-camping-vest' }
        , { id:'icofont-direction-sign' }
        , { id:'icofont-hill-side' }
        , { id:'icofont-hill' }
        , { id:'icofont-hotel' }
        , { id:'icofont-island-alt' }
        , { id:'icofont-island' }
        , { id:'icofont-sandals-female' }
        , { id:'icofont-sandals-male' }
        , { id:'icofont-travelling' }
    ]
//-// Ico icon mixin
mixin IcoContent(items)
    each item in items 
        .col-xxl-2.col-lg-3.col-sm-4
            .card.d-flex.align-items-center.flex-column.default-border
                i.icofont(class=item.id)
                .flex-grow-1
                    h6=item.id
// Ico icon
.card
    .card-header.pb-0.d-flex.justify-content-between.align-items-center
        h4 Travel Icon
    .card-body
        .row.icon-event.iconly-icons.icon-lists
            +IcoContent(ICO_ITEM27)