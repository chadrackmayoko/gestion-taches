//-// Emoticon
-
    var ICO_ITEM3 =[
        { id:'icofont-angry' }
        , { id:'icofont-astonished' }
        , { id:'icofont-confounded' }
        , { id:'icofont-confused' }
        , { id:'icofont-crying' }
        , { id:'icofont-dizzy' }
        , { id:'icofont-expressionless' }
        , { id:'icofont-heart-eyes' }
        , { id:'icofont-laughing' }
        , { id:'icofont-nerd-smile' }
        , { id:'icofont-open-mouth' }
        , { id:'icofont-rage' }
        , { id:'icofont-rolling-eyes' }
        , { id:'icofont-sad' }
        , { id:'icofont-simple-smile' }
        , { id:'icofont-slightly-smile' }
        , { id:'icofont-smirk' }
        , { id:'icofont-stuck-out-tongue' }
        , { id:'icofont-wink-smile' }
        , { id:'icofont-worried' }
    ]
//-// Ico icon mixin
mixin IcoContent(items)
    each item in items 
        .col-xxl-2.col-lg-3.col-sm-4
            .card.d-flex.align-items-center.flex-column.default-border
                i.icofont(class=item.id)
                .flex-grow-1
                    h6=item.id
// Ico icon
.card
    .card-header.pb-0.d-flex.justify-content-between.align-items-center
        h4 Emoticon
    .card-body
        .row.icon-event.iconly-icons.icon-lists
            +IcoContent(ICO_ITEM3)